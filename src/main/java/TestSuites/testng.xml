<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite  name="All Test Suite">
    <listeners>
        <listener class-name="org.tests.Listeners"></listener>
    </listeners>
    <test thread-count="5" verbose="2" preserve-order="true" name="Submit Order Test">
        <classes>
            <class name="org.tests.SubmitTest"/>
        </classes>
    </test>

    <test thread-count="5" verbose="2" preserve-order="true" name="Error Validations Test">
        <classes>
            <class name="org.tests.ErrorValidations"/>
        </classes>
    </test>


</suite>